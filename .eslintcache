porary có một lớp phủ mờ hiện ra cho đến khi bạn chọn xong thì Drawer mới đóng lại\r\n        >\r\n          {content}\r\n        </Drawer>\r\n      </Hidden>\r\n\r\n      {/* đây là giao diện desktop */}\r\n      <Hidden mdDown>\r\n        <Drawer anchor=\"left\" classes={{ paper: classes.desktopDrawer }} open variant=\"persistent\">\r\n          {content}\r\n        </Drawer>\r\n      </Hidden>\r\n    </>\r\n  );\r\n};\r\n\r\nNavBar.propTypes = {\r\n  onMobileClose: PropTypes.func,\r\n  openMobile: PropTypes.bool\r\n};\r\n\r\nNavBar.defaultProps = {\r\n  onMobileClose: () => { },\r\n  openMobile: false\r\n};\r\n\r\nexport default NavBar;","C:\\Users\\Nguyen Phu\\Desktop\\MovieReact\\movie-reactjs\\src\\components\\Header\\index.js",[],"C:\\Users\\Nguyen Phu\\Desktop\\MovieReact\\movie-reactjs\\src\\layouts\\AdminLayout\\Logo.js",[],"C:\\Users\\Nguyen Phu\\Desktop\\MovieReact\\movie-reactjs\\src\\api\\moviesApi.js",[],"C:\\Users\\Nguyen Phu\\Desktop\\MovieReact\\movie-reactjs\\src\\api\\usersApi.js",[],"C:\\Users\\Nguyen Phu\\Desktop\\MovieReact\\movie-reactjs\\src\\components\\Header\\style.js",[],"C:\\Users\\Nguyen Phu\\Desktop\\MovieReact\\movie-reactjs\\src\\api\\axiosClient.js",[],"C:\\Users\\Nguyen Phu\\Desktop\\MovieReact\\movie-reactjs\\src\\layouts\\AdminLayout\\NavBar\\NavItem.js",[],"C:\\Users\\Nguyen Phu\\Desktop\\MovieReact\\movie-reactjs\\src\\components\\News\\index.js",["333","334","335","336","337","338","339","340","341","342","343","344","345","346","347","348","349","350","351","352","353","354","355","356","357","358","359","360","361","362","363","364","365","366","367","368","369"],"import React from 'react';\r\nimport PropTypes from 'prop-types';\r\nimport { makeStyles } from '@material-ui/core/styles';\r\nimport AppBar from '@material-ui/core/AppBar';\r\nimport Tabs from '@material-ui/core/Tabs';\r\nimport Tab from '@material-ui/core/Tab';\r\nimport Typography from '@material-ui/core/Typography';\r\nimport Box from '@material-ui/core/Box';\